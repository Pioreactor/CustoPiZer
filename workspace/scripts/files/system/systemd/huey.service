[Unit]
Description=Huey service for PioreactorUI
After=network.target firstboot.service
WantedBy=lighttpd.service

[Service]
User=pioreactor
WorkingDirectory=/var/www/pioreactorui
ExecStartPre=/bin/bash /usr/local/bin/create_diskcache.sh
ExecStart=huey_consumer tasks.huey -s 60 -n -b 1.0 -w 2 -f -C
ExecStartPost=pio log -m "%n successful" -n systemd --local-only
StandardOutput=null
StandardError=null

[Install]
WantedBy=multi-user.target